#include <Servo.h>

#define SERVO 9

Servo servo;

void setup()
{
  servo.attach(SERVO);
  servo.write(0);
  delay(1000);
  servo.write(180);
  delay(1000);
  servo.write(90);
  delay(1000);
  servo.write(0);
  delay(3000);
  servo.write(180);
  delay(360);
  servo.write(90);
  delay(180);
  servo.write(0);
}

void loop()
{
}